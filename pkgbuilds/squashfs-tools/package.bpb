name=squashfs-tools
version=4.5.1
real_version=0
source=https://github.com/plougher/squashfs-tools/archive/refs/tags/4.5.1.zip
dependencies=
builddeps=[make][gcc]
description=This package provides squashfs tools
build={
	unzip 4.5.1.zip
	cd squashfs-tools-4.5.1
	cd squashfs-tools
	make -j$(nproc)
	make INSTALL_PREFIX=$PKG_INSTALL_DIR/usr/local install
}