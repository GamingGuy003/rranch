name=which
version=2.21
real_version=0
source=https://ftp.gnu.org/gnu/which/which-2.21.tar.gz
dependencies=
builddeps=
description=The GNU which package
build={
	cd which-2.21
	./configure --prefix=/usr &&
	make -j$(nproc)
	make DESTDIR=$PKG_INSTALL_DIR install

	rm -fv $PKG_INSTALL_DIR/usr/share/info/dir
}